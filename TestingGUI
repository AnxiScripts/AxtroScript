local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/ConManDeveloper/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("My Kavo UI", "DarkTheme")

-- Main Tab
local MainTab = Window:NewTab("Main")
local MainSection = MainTab:NewSection("Controls")

MainSection:NewButton("Say Hello", "Prints to the console", function()
    print("Hello, world!")
end)

MainSection:NewToggle("Enable Speed", "Toggles speed setting", function(state)
    print("Speed Enabled:", state)
end)

MainSection:NewSlider("WalkSpeed", "Changes your speed", 100, 16, function(value)
    local player = game.Players.LocalPlayer
    if player and player.Character and player.Character:FindFirstChild("Humanoid") then
        player.Character.Humanoid.WalkSpeed = value
    end
end)
