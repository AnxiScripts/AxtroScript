local WindUI = loadstring(game:HttpGet("https://github.com/Footagesus/WindUI/releases/latest/download/main.lua"))()

WindUI:SetNotificationLower(true)

-- Optional: Discord link to copy
local discordLink = "https://discord.gg/FuHV5nFhcZ"

-- Show popup asking to join Discord
WindUI:Popup({
    Title = "Join Our Discord?",
    Icon = "rbxassetid://129260712070622",
    IconThemed = true,
    Content = "Would you like to join our Discord server for updates?",
    Buttons = {
        {
            Title = "No",
            Variant = "Secondary",
            Callback = function() end,
        },
        {
            Title = "Yes",
            Variant = "Primary",
            Callback = function()
                if setclipboard then
                    setclipboard(discordLink)
                    WindUI:Notify({
                        Title = "Copied!",
                        Content = "Discord invite link copied to clipboard.",
                        Duration = 3
                    })
                else
                    WindUI:Notify({
                        Title = "Clipboard Failed",
                        Content = "Clipboard not supported in this environment.",
                        Duration = 3
                    })
                end
            end,
        }
    }
})

-- Wait 3 seconds before showing the main UI
task.delay(3, function()
    local Window = WindUI:CreateWindow({
        Title = "Axtro | Testing",
        Icon = "rbxassetid://129260712070622",
        IconThemed = true,
        Author = "Axtro Hub",
        Folder = "CloudHub",
        Size = UDim2.fromOffset(580, 460),
        Transparent = true,
        Theme = "Dark",
        User = {
            Enabled = true,
            Callback = function() print("User profile clicked") end,
            Anonymous = true
        },
    })

    -- Create a Tab
    local Tab = Window:Tab({
        Title = "HOME",
        Icon = "crown",
        Locked = false,
    })

    local Section = Tab:Section({
        Title = "SCRIPT INFORMATION",
        Content = [[
This script was created by Crowned.
DISCLAIMER: Please join the Discord
server to know if the script is Undetected.
DISCLAIMER: If anything breaks, reload
the script.
More improvements coming soon!
        ]],
        TextXAlignment = "Left",
        TextSize = 17,
    })

    -- Automatically switch to first tab
    Window:SelectTab(1)
end)
